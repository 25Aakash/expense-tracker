{
  "expo": {
    "name": "DailyCashBook",
    "slug": "dailycashbook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "A comprehensive expense tracking app for personal and team financial management with multi-role support.",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#6366f1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expensetracker.pro",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366f1"
      },
      "package": "com.expensetracker.pro",
      "versionCode": 1,
      "permissions": [],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "a878dfa8-9c1b-4119-8893-6b8c216ddfa2"
      },
      "supportEmail": "dailycashbook3@gmail.com",
      "supportUrl": "https://github.com/25Aakash/dailycashbook",
      "privacyPolicyUrl": "https://github.com/25Aakash/dailycashbook/blob/main/PRIVACY_POLICY.md",
      "termsOfServiceUrl": "https://github.com/25Aakash/dailycashbook/blob/main/TERMS_OF_SERVICE.md"
    },
    "owner": "kaakash",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a878dfa8-9c1b-4119-8893-6b8c216ddfa2"
    }
  }
}
